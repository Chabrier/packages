<dynamics>
<dynamic library="LotkaVolterraY" package="vle.ode_test" name="dynLotkaVolterraY"/>
<dynamic library="Perturb" package="vle.ode_test" name="dynPerturb"/>
</dynamics>
<experiment combination="linear" name="ExtUpLV">
<conditions>
<condition name="simulation_engine">
<port name="begin">
<double>0.0</double>
</port>  
<port name="duration">
<double>0.5</double>
</port> 
</condition>
<condition name="condEuler">
 <port name="method">
<string>euler</string>
</port>
 
<port name="time_step">
     <double>0.01</double>
    </port>
   </condition>
<condition name="condExtInit">
 <port name="message">
<map>
      <key name="name">
       <string>X</string>
      </key>
      <key name="value">
       <double>10.000000000000000</double>
      </key>
     </map>
</port>
 <port name="sendTime">
<double>0.000000000000000</double>
</port>
</condition>
<condition name="condExtUp">
 <port name="message">
<map>
      <key name="name">
       <string>X</string>
      </key>
      <key name="value">
       <double>1.000000000000000</double>
      </key>
     </map>
</port>
 <port name="sendTime">
<double>0.405000000000000</double>
</port>
</condition>
<condition name="condLV_Y">
 <port name="alpha">
<double>1.500000000000000</double>
</port>
 <port name="beta">
<double>1.000000000000000</double>
</port>
 <port name="delta">
<double>3.000000000000000</double>
</port>
 <port name="gamma">
<double>1.000000000000000</double>
</port>
 
<port name="init_value_Y">
     <double>5</double>
    </port>
   </condition>
