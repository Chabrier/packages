<views>
<outputs>
<output location="" plugin="file" format="local" package="vle.output" name="view">
<map>
      <key name="julian-day">
       <boolean>false</boolean>
      </key>
      <key name="locale">
       <string>C</string>
      </key>
      <key name="type">
       <string>csv</string>
      </key>
     </map>
    </output>

</outputs>
<observables>
<observable name="obs">
<port name="X">
 <attachedview name="view"/>
</port>

<port name="Y">
 <attachedview name="view"/>
</port>

</observable>

</observables>
<view output="view" type="timed" name="view" timestep="0.010000000000000"/>

</views>
</experiment>
